<h1>Items</h1>
<a href="/new">Add New Item</a>
<ul>
  <% items.forEach(item => { %>
    <li>
      <%= item.name %> (Quantity: <%= item.quantity %>)
      <a href="/<%= item.id %>/edit">Edit</a>
      <form action="/<%= item.id %>?_method=DELETE" method="POST">
        <button type="submit">Delete</button>
      </form>
    </li>
  <% }) %>
</ul>